{
    "name": "oesteve/transformer",
    "description": "An easy, fast and scalable way to build DTO transformers",
    "version": "0.0.1",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "oesteve",
            "email": "oscar@oscaresteve.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Oesteve\\Transformer\\": "lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Oesteve\\Tests\\Transformer\\": "tests"
        }
    },
    "scripts": {
        "php-cs-fixer": "php-cs-fixer fix",
        "phpstan": "vendor/bin/phpstan analyse --level 8 lib"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.88",
        "phpunit/phpunit": "^9.5",
        "symfony/dependency-injection": "^5.3",
        "symfony/http-kernel": "^5.3",
        "friendsofphp/php-cs-fixer": "^3.3"
    }
}
