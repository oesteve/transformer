{
    "name": "oesteve/transformer",
    "description": "An easy, fast and scalable way to build DTO transformers",
    "version": "0.0.1",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "oesteve",
            "email": "oscar@oscaresteve.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Oesteve\\Transformer\\": "lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Oesteve\\Tests\\Transformer\\": "tests"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse --level 8 lib",
        "cs-fixer": "tools/php-cs-fixer/vendor/bin/php-cs-fixer --allow-risky=yes fix",
        "phpunit": "phpunit"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.88",
        "phpunit/phpunit": "^9.5",
        "symfony/dependency-injection": "^5.3",
        "symfony/http-kernel": "^5.3",
        "symfony/framework-bundle": "^5.3",
        "psr/log": "^1.1",
        "psr/cache": "^2.0",
        "symfony/cache": "^5.3"
    },
    "require": {
        "symfony/property-access": "^5.3"
    }
}
